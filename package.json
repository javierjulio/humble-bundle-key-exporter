{
  "name": "humble-bundle-key-exporter",
  "version": "1.0.0",
  "description": "A bookmarklet to export [your unredeemed HumbleBundle Keys](https://www.humblebundle.com/home/keys) forthat once parsed will download that data in a formatted JSON file. This is meant for personal use.",
  "main": "src/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "watch": "./node_modules/.bin/rollup -c -w",
    "eslint": "./node_modules/.bin/eslint .",
    "test": "echo 'No tests.'",
    "pretest": "npm run build",
    "default": "npm run build",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javierjulio/humble-bundle-key-exporter.git"
  },
  "keywords": [
    "javascript",
    "humble bundle",
    "bookmarklet",
    "keys",
    "export",
    "exporter",
    "scraper"
  ],
  "devDependencies": {
    "eslint": "^7.27.0",
    "rollup": "^2.53.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-terser": "~7.0"
  },
  "author": "Javier Julio (http://javierjulio.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javierjulio/humble-bundle-key-exporter/issues"
  },
  "homepage": "https://github.com/javierjulio/humble-bundle-key-exporter"
}
